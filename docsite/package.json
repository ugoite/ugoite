{
	"name": "ugoite-docsite",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "astro dev --host",
		"build": "astro build",
		"preview": "astro preview --host",
		"lint": "biome ci astro.config.mjs package.json tsconfig.json biome.json src/env.d.ts",
		"format": "biome format --write .",
		"format:check": "biome ci astro.config.mjs package.json tsconfig.json biome.json src/env.d.ts",
		"typecheck": "astro check",
		"validate": "bun run lint && bun run typecheck"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.6",
		"@astrojs/tailwind": "^6.0.0",
		"astro": "^5.13.5",
		"marked": "^16.4.1",
		"prismjs": "1.30.0",
		"tailwindcss": "^3.4.17",
		"yaml": "^2.8.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.5",
		"@types/node": "^24.3.0",
		"typescript": "^5.9.3"
	}
}
